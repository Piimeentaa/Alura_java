
public class TestaVariaveis {

	public static void main(String[] args) {
		System.out.println("Ola novo teste");
		
		int idade = 37;
		System.out.println("Minha idade é: " + idade );
		
		idade = 30 + 7;
		System.out.println("Minha idade é:" + idade);
		
		idade = (5 * 6) + 7;
		System.out.println("Minha idade é: " + idade);
		

	}

}
